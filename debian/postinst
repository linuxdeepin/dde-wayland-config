#!/bin/bash

#[ ! -d /usr/share/xsessions ] && mkdir -p /usr/share/xsessions 
#[ -f /usr/share/wayland-sessions/deepin.desktop ] && mv -f /usr/share/wayland-sessions/deepin.desktop /usr/share/xsessions/deepin.desktop
